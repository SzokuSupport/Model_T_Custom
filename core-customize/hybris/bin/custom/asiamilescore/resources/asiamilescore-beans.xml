<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<enum
			class="de.hybris.platform.acceleratorservices.controllers.page.PageType">
		<value>OrderHistory</value>
		<value>OrderErrorGeneral</value>
		<value>OrderErrorBalance</value>
		<value>CheckOutStepOne</value>
	</enum>

	 <bean class="de.hybris.platform.commerceservices.order.CommerceCartModification">
		<property name="entryNumber" type="java.lang.Integer"/>
	</bean>
	
	<bean class="de.hybris.platform.commerceservices.service.data.CommerceCheckoutParameter">
		<property name="orderVersion" type="String"/>
  	</bean>

	<bean class="de.hybris.platform.oidcsinglesignon.data.TokenRelation">
		<property name="loginToken" type="String"/>
		<property name="accessToken" type="String"/>
		<property name="idToken" type="String"/>
		<property name="tokenType" type="String"/>
		<property name="expiresIn" type="String"/>
		<property name="refreshToken" type="String"/>
	</bean>
</beans>
