                   xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/action-definition.xsd">
    <name>DownLoad - Home delivery </name>
    <description>Confirm Export</description>
    <author>hybris</author>
    <version>0.1</version>

    <actionClassName>com.asiamiles.backoffice.actions.consignment.HomeDeliveryReportAction</actionClassName>
    <inputType>java.util.Map</inputType>
    
    <iconUri>icons/homeDelivery-default.png</iconUri>
    <iconDisabledUri>icons/homeDelivery-disable.png</iconDisabledUri>
    <iconHoverUri>icons/homeDelivery-hover.png</iconHoverUri>
    <settings>
    	  <setting key="viewMode" default-value="iconOnly"/>
        <setting key="actionType" default-value="CONFIRM_EXPORT"/>
   </settings>
